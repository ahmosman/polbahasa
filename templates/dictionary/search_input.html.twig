<form action="{{ path('dictionary') }}" method="get" autocomplete="off" id="searching-form">
    <div class="search-input">
        <input type="text" name="q" placeholder="{{ 'search.placeholder'|trans }}" value="{{ app.request.get('q') }}">
        <button type="submit" class="search-submit-btn fas fa-search" title="{{ 'search.search-btn'|trans }}"></button>
    </div>
    <div class="suggestions-div hidden" style="height: 200px">
        <ul class="search-suggestions-ul suggestions-ul">
        </ul>
    </div>
</form>
{% block javascripts %}
    {{ encore_entry_script_tags('search_suggestions') }}
{% endblock %}